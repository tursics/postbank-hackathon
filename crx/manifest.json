{
  "manifest_version": 2,
  "name": "Postbank Hackathon",
  "description": "This extension enhance the Postbank immobilien.postbank.de website",
  "version": "1.0.0.1",
  "permissions": ["tabs", "http://*/*"],
  "icons": { "16": "icon16.png", "48": "icon48.png", "128": "icon128.png" },
  "content_scripts" : [{
        "matches" : [ "https://immobilien.postbank.de/*"],
        "js" : ["immobilien.postbank.js"]
    }]
}
